        "import requests\n",
        "def search_models(query, limit=5):\n",
        "  url = f\"https://huggingface.co/api/models?search={query}&limit={limit}\"\n",
        "  response = requests.get(url)\n",
        "  return response.json()\n",
        "\n",
        "models = search_models(\"llama\")\n",
        "for m in models:\n",
        "  print(m['id'])"
      ]
